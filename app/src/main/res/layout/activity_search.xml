        android:ems="10"
        android:hint="              Task , Projects and more"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/imageView44"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <ImageView
        android:id="@+id/imageView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="76dp"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toTopOf="@+id/navigationView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText10"
        app:srcCompat="@drawable/searchingdata"
        android:clickable="true"
        android:contentDescription="@string/clickable_image"
        android:focusable="true"/>

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="31dp"
        android:layout_height="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.281"
        app:srcCompat="@drawable/search"
        android:clickable="true"
        android:contentDescription="@string/clickable_image"
        android:focusable="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>